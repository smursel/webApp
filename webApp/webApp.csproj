  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\ControlListings.cshtml" />
  </ItemGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="Pages\ControlListings.cshtml" />
  </ItemGroup>

</Project>

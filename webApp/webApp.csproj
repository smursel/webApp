    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="External\config\GoogleAPIKey.json" />
    <Content Remove="Pages\ControlListings.cshtml" />
  </ItemGroup>

  <ItemGroup>
    <None Include="External\config\GoogleAPIKey.json" />
